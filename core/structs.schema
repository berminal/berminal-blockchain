struct UTXO  {
  BirthTxHash []byte
  Value int64
  Script string
}

struct TxInput {
   TxHash []byte
   UnlockScript string
   StateHash []byte
}

struct Tx {
   Version int32
   Inputs []TxInput
   Outputs []UTXO
   Time int64
}

struct TxPoolRaw {
    Txs []Tx
    TxHash [][]byte
}

struct BlockHead {
   Version int8
   SuperHash []byte
   TreeHash []byte
   Info []byte
   Time    int64
}

struct Block {
   Version   int32
   Head      BlockHead
   Content   []byte
}